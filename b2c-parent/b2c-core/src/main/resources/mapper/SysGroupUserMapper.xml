<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.javamaster.b2c.core.mapper.SysGroupUserMapper">
    <resultMap id="BaseResultMap" type="org.javamaster.b2c.core.entity.SysGroupUser">
        <result column="group_code" jdbcType="CHAR" property="groupCode"/>
        <result column="username" jdbcType="VARCHAR" property="username"/>
    </resultMap>
    <insert id="insert" parameterType="org.javamaster.b2c.core.entity.SysGroupUser">
        insert into sys_group_user (group_code, username)
        values (#{groupCode,jdbcType=CHAR}, #{username,jdbcType=VARCHAR})
    </insert>
    <select id="selectAll" resultMap="BaseResultMap">
        select group_code, username
        from sys_group_user
    </select>
</mapper>